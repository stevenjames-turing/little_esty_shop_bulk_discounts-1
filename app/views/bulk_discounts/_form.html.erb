<%= form_with model: model, method: method, url: url, class: 'merchant_bulk_discount_form', local: true do |f| %>
  <%= f.label :discount %>
  <%= f.number_field :discount, step: 0.01 %><br/><br/>
  <%= f.label :threshold %>
  <%= f.number_field :threshold %><br/><br/>
  <%= f.submit 'Submit'%>
<% end %>